{
    "name": "salahmyn/laravel-image-resize",
    "description": "Dynamically resize an image and returns the URL using Intervention and Storage",
    "homepage": "https://github.com/salahmyn/laravel-image-resize",
    "keywords":
    [
        "image",
        "resize",
        "laravel"
    ],
    "license": "MIT",
    "authors": [
        { 
            "name": "Salah Al-Nawah",
            "email": "salahmyn@gmail.com"
        },
        {
            "name": "Mark Padilla",
            "email": "mark.padilla@maximum.com"
        },
        {
            "name": "Jonas Van Assche",
            "email": "jonas.van.assche@maximum.com"
        }
    ],
    "require": {
        "php" : "^7.2",
        "intervention/image": "^2.4",
        "illuminate/support": "^6.0",
        "illuminate/config": "~6.0",
        "illuminate/cache": "^6.0",
        "league/flysystem": "~1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5",
        "orchestra/testbench": "^3.5"
    },
    "autoload": {
        "psr-4": {
            "Mxmm\\ImageResize\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mxmm\\ImageResize\\Tests\\": "tests"
        }
    },
    "extra":{
        "laravel":{
            "providers":[
                "Mxmm\\ImageResize\\ImageResizeServiceProvider"
            ],
            "aliases":{
                "ImageResize": "Mxmm\\ImageResize\\Facade"
            }
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
